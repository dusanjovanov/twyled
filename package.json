{
  "name": "twyled",
  "version": "0.0.4",
  "description": "React UI library inspired by Tailwind + Styled System",
  "keywords": [
    "react",
    "ui",
    "primitives",
    "components",
    "tailwind",
    "styled-system"
  ],
  "author": "Dusan Jovanov",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/dusanjovanov/twyled"
  },
  "main": "dist/index.cjs",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "default": "./dist/index.modern.js"
  },
  "types": "dist/index.d.ts",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "dev": "microbundle watch",
    "build": "rm -rf dist && microbundle"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "microbundle": "^0.15.1"
  },
  "peerDependencies": {
    "react": ">=16.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5"
  }
}
